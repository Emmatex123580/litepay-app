<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="referalsSixVM" type="com.litepayapp.app.modules.referalssix.data.viewmodel.ReferalsSixVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearReferalsSix"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_bluegray_900_14x14"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_900_14x14"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKYC"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_123pxh"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{referalsSixVM.referalsSixModel.txtKYC}"
                    style="@style/txtPoppinsmedium16" tools:text="@string/lbl_kyc"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageView android:id="@+id/imageSettings"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_66pxh"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_50pxv"
            android:layout_width="@dimen/_66pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_settings_black_900"
            tools:ignore="ContentDescription" tools:src="@drawable/img_settings_black_900"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtSelectPassport"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{referalsSixVM.referalsSixModel.txtSelectPassport}"
            style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_select_passport"/>
        <TextView android:ellipsize="end" android:id="@+id/txtLanguage"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{referalsSixVM.referalsSixModel.txtLanguage}"
            style="@style/txtPoppinsregular13" tools:text="@string/msg_max_size_of_1"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end"
            android:hint="@string/lbl_first_name"
            android:id="@+id/etFrame627042" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627042Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end"
            android:hint="@string/lbl_middle_name"
            android:id="@+id/etFrame627196" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627196Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_surname"
            android:id="@+id/etFrame627197" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627197Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_nin_bvn"
            android:id="@+id/etFrame627198" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627198Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end"
            android:hint="@string/lbl_state_of_origin"
            android:id="@+id/etFrame627199" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_6pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_9pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627199Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_5"
            android:ellipsize="end"
            android:hint="@string/lbl_lg_of_origin"
            android:id="@+id/etFrame627200" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_6pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_12pxh"
            android:paddingTop="@dimen/_9pxv" android:singleLine="true"
            android:text="@={referalsSixVM.referalsSixModel.etFrame627200Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <Spinner
            android:background="@drawable/layer_list_bg_gray_100_radius_5"
            android:gravity="center_horizontal"
            android:id="@+id/spinnerFrame627042One"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumngender"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStyle_2">
            <TextView android:ellipsize="end"
                android:id="@+id/txtGender"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_4pxv"
                android:paddingStart="@dimen/_8pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@{referalsSixVM.referalsSixModel.txtGender}"
                style="@style/txtSolidRounded_2" tools:text="@string/lbl_male"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGenderOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_4pxv"
                android:paddingStart="@dimen/_8pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@{referalsSixVM.referalsSixModel.txtGenderOne}"
                style="@style/txtSolidRounded_2" tools:text="@string/lbl_female"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSubmit"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_40pxv"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="@dimen/_233pxh"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_8pxv" android:singleLine="true"
            android:text="@string/lbl_submit"
            android:textAllCaps="false"
            style="@style/btnSolidRounded_10" tools:text="@string/lbl_submit"/>
