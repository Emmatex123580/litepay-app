<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="contactUsPageOneVM" type="com.litepayapp.app.modules.contactuspageone.data.viewmodel.ContactUsPageOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearContactuspage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_bluegray_900_14x14"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_900_14x14"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContactUs"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_101pxh"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtContactUs}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_contact_us"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowcarbontime"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCarbontime"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_carbontime"
                tools:ignore="ContentDescription" tools:src="@drawable/img_carbontime"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.79" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtTime}"
                style="@style/txtPoppinsmedium16" tools:text="@string/lbl_8am_10pm"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowcall"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCall"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_call_black_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_call_black_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMobileNo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_weight="0.88" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtMobileNo}"
                style="@style/txtPoppinsmedium16" tools:text="@string/lbl_234_8142920154"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlinkedin"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLinkedin"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_23pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_linkedin"
                tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
            <TextView android:id="@+id/txtNo10calabar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_weight="0.93" android:layout_width="0dp"
                android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtNo10calabar}"
                style="@style/txtPoppinsmedium16" tools:text="@string/msg_no_10_calabar"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtOR"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtOR}"
            style="@style/txtPoppinsbold20" tools:text="@string/lbl_or"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtSendusamessa"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_75pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtSendusamessa}"
            style="@style/txtPoppinsmedium16" tools:text="@string/msg_send_us_a_messa"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_17"
            android:ellipsize="end" android:hint="@string/lbl_name"
            android:id="@+id/etFrame627083" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_8pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={contactUsPageOneVM.contactUsPageOneModel.etFrame627083Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_17"
            android:ellipsize="end" android:hint="@string/lbl_email2"
            android:id="@+id/etFrame627216"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_8pxh"
            android:paddingTop="@dimen/_7pxv" android:singleLine="true"
            android:text="@={contactUsPageOneVM.contactUsPageOneModel.etFrame627216Value}"
            android:textColorHint="@color/bluegray_900_7e"
            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsearchTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_3">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtSearchTwo"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_109pxv"
                android:layout_marginEnd="@dimen/_255pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtSearchTwo}"
                style="@style/txtPoppinsregular12_4" tools:text="@string/lbl_message"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStacknext"
            android:layout_height="@dimen/_69pxv"
            android:layout_marginTop="@dimen/_7pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnnext"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylepurple_4">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtNext"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_65pxh"
                    android:layout_marginStart="@dimen/_65pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtNext}"
                    style="@style/txtPoppinsmedium12" tools:text="@string/lbl_send_message"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.28"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomeOne"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtHomeOne}"
                        style="@style/txtPoppinsregular10_1" tools:text="@string/lbl_home"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnsort"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.29"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageSort"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_sort"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_sort"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPricing"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtPricing}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_pricing"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnmaximize"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.33"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageMaximize"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_maximize"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_maximize"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtReferrals"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtReferrals}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_referrals"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnlock"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.1"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{contactUsPageOneVM.contactUsPageOneModel.txtProfile}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_profile"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
