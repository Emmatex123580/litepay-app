<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="bettingTwoRowModel" type="com.litepayapp.app.modules.bettingtwo.data.model.BettingTwoRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowcheckmark"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStyle_1">
        <LinearLayout android:id="@+id/linearRowsportybet"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_weight="0.85" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImageEighteen"
                android:layout_height="@dimen/_33pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_image18"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSportyBet"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight="0.7" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{bettingTwoRowModel.txtSportyBet}"
                style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_sportybet"/>
        </LinearLayout>
        <ImageButton
            android:background="@drawable/rectangle_border_purple_501"
            android:id="@+id/btnCheckmark"
            android:layout_height="@dimen/_24pxh"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginStart="@dimen/_163pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_24pxh"
            android:paddingHorizontal="@dimen/_6pxh"
            android:paddingVertical="@dimen/_6pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"/>
    </LinearLayout>
</layout>
