<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpFourVM" type="com.litepayapp.app.modules.signupfour.data.viewmodel.SignUpFourVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignupFour"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackvectornineteen"
            android:layout_height="@dimen/_227pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageVectorNineteen"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_112pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_vector19}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector19"/>
            <LinearLayout android:id="@+id/linearColumnlock"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_87pxh"
                android:layout_marginStart="@dimen/_87pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_80pxh"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_width="@dimen/_80pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtWelcomebackNw"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent"
                    android:text="@{signUpFourVM.signUpFourModel.txtWelcomebackNw}"
                    style="@style/txtPoppinsbold20" tools:text="@string/msg_welcome_back_nw"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStackgroupfortythree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_42pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_47pxv" android:layout_width="@dimen/_320pxh">
            <FrameLayout android:id="@+id/frameStackeye"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_33pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_320pxh" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageEye"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye_bluegray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye_bluegray_900"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGroupFortyThree"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_1pxv"
                android:paddingStart="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{signUpFourVM.signUpFourModel.txtGroupFortyThree}"
                style="@style/txtSolid" tools:text="@string/lbl_password"/>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtForgotpassword"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUpFourVM.signUpFourModel.txtForgotpassword}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_forgot_password"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSignIn"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="@dimen/_278pxh"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_10pxv" android:singleLine="true"
            android:text="@string/lbl_sign_in"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_sign_in"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowfingerprint"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageFingerprint"
                android:layout_height="@dimen/_29pxh"
                android:layout_width="@dimen/_26pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_fingerprint_bluegray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_fingerprint_bluegray_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFingerprintOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.75" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{signUpFourVM.signUpFourModel.txtFingerprintOne}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_fingerprint"/>
        </LinearLayout>
        <ImageView android:gravity="start"
            android:id="@+id/imageVectorTwenty"
            android:layout_height="@dimen/_155pxv"
            android:layout_marginTop="@dimen/_129pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector20"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector20"/>
    </LinearLayout>
</layout>
