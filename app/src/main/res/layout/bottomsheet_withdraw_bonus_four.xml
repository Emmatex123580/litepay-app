<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="withdrawBonusFourVM" type="com.litepayapp.app.modules.withdrawbonusfour.data.viewmodel.WithdrawBonusFourVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearWithdrawBonus"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnconfirmtransac"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <LinearLayout android:id="@+id/linearRowconfirmtransac"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmTransac"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtConfirmTransac}"
                    style="@style/txtPoppinssemibold18" tools:text="@string/msg_confirm_transac"/>
                <ImageView android:id="@+id/imageClose"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_110pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_21pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close_red_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close_red_a700"/>
            </LinearLayout>
            <TextView android:gravity="start"
                android:id="@+id/txtYouareaboutt"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtYouareaboutt}"
                style="@style/txtPoppinsregular14_2" tools:text="@string/msg_you_are_about_t3"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEnterPINtoco"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtEnterPINtoco}"
                style="@style/txtPoppinssemibold15" tools:text="@string/msg_enter_pin_to_co"/>
            <com.mukesh.OtpView android:focusableInTouchMode="true"
                android:id="@+id/otpViewOtpview"
                android:inputType="number"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content"
                android:textColor="@color/black_900"
                android:textSize="@dimen/_15pxh" app:OtpItemCount="4"
                app:OtpItemHeight="@dimen/_27pxv"
                app:OtpItemSpacing="@dimen/_20pxh"
                app:OtpItemWidth="@dimen/_31pxh"
                app:OtpLineColor="@color/purple_500" app:OtpViewType="rectangle"/>
            <LinearLayout android:id="@+id/linearColumnframe627146"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_47pxh"
                android:layout_marginStart="@dimen/_47pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="start"
                    android:id="@+id/recyclerListframe627146"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_12pxh}" tools:itemCount="2" tools:listitem="@layout/row_listframe627148"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowframe627153"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFrame627153"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.36"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_11pxh"
                        android:paddingStart="@dimen/_11pxh"
                        android:paddingTop="@dimen/_3pxv"
                        android:singleLine="true"
                        android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtFrame627153}"
                        style="@style/txtSolidRounded_1" tools:text="@string/lbl_9"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFrame627155"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_weight="0.36"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_11pxh"
                        android:paddingStart="@dimen/_11pxh"
                        android:paddingTop="@dimen/_3pxv"
                        android:singleLine="true"
                        android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtFrame627155}"
                        style="@style/txtSolidRounded_1" tools:text="@string/lbl_0"/>
                    <ImageView android:id="@+id/imageCloseOne"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_red_a700_18x24"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_red_a700_18x24"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtOR"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtOR}"
                style="@style/txtPoppinsmedium12_3" tools:text="@string/lbl_or"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUseFingerprint"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{withdrawBonusFourVM.withdrawBonusFourModel.txtUseFingerprint}"
                style="@style/txtPoppinssemibold12_2" tools:text="@string/lbl_use_fingerprint"/>
        </LinearLayout>
    </LinearLayout>
</layout>
